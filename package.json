{
  "name": "mugiwara",
  "version": "0.0.1",
  "description": "blazing fast minimal css-in-js solution",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "yarn build",
    "lint": "node ./scripts/eslint/index.js",
    "linc": "node ./scripts/eslint/only-changed.js",
    "build": "node scripts/rollup/build.js",
    "clean-node-modules": "find ./ -name 'node_modules' -exec rm -rf '{}' +",
    "prepublishOnly": "yarn test && yarn build:prod",
    "test": "yarn lint && yarn prettier:stat && yarn jest:ci",
    "jest": "NODE_ENV=TEST jest --testPathIgnorePatterns \"(__fixtures__|matcher.js)\"",
    "jest:ci": "NODE_ENV=TEST jest --no-cache --testPathIgnorePatterns \"(__fixtures__|matcher.js)\" --ci --runInBand",
    "prettier": "node ./scripts/prettier/index.js write-changed",
    "prettier:all": "node ./scripts/prettier/index.js write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphamorim/mugiwara.git"
  },
  "keywords": [],
  "author": "Raphael Amorim <rapha850@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raphamorim/mugiwara/issues"
  },
  "homepage": "https://github.com/raphamorim/mugiwara#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.2",
    "cli-table": "^0.3.1",
    "eslint": "^4.18.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.2",
    "jest": "^22.4.2",
    "minimatch": "^3.0.4",
    "prettier": "^1.5.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-optimize-js": "^0.0.4",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": true,
    "resetModules": true
  }
}
